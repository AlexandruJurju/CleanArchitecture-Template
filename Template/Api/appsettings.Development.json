{
  "ConnectionStrings": {
    "Database": "Host=template.db;Port=5432;Database=template;Username=postgres;Password=postgres;Include Error Detail=true",
    "Cache": "template.redis:6379"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args": {
          "ServerUrl": "http://template.seq:5341"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId",
      "WithCorrelationId"
    ]
  },
  "Jwt": {
    "Secret": "secret-value-that-should-be-in-user-secrets-lmao",
    "Issuer": "template",
    "Audience": "users",
    "ExpirationInMinutes": 60
  },
  "BackgroundJobs": {
    "Outbox": {
      "Schedule": "0/15 * * * * *"
    }
  }
}
